{
  "compilerOptions": {
    "lib": [ "es6" ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "pretty": true,
    "removeComments": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  }
}
